{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET","HEAD","PUT","PATCH","POST","OPTIONS","DELETE"]
      }
    ],

    "git": {
      "deploymentEnabled": {
        "main": true
      }
    }
    
  }
  