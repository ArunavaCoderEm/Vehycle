{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",  
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*"]
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.ts",
        "methods": ["GET", "HEAD", "PUT", "PATCH", "POST", "OPTIONS", "DELETE"]
      }
    ],
    "git": {
      "deploymentEnabled": {
        "main": true
      }
    }
  }
  
  